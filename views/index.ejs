<div class="content">
  <div class="input-container">
    <a href="/new"><button class="add-message-btn">Add Message</button></a>
    <p>Welcome to the Mini Messageboard</p>
  </div>
  <div class="message-display">
    <% messages.forEach(message => { %>
    <h3><%= message.user %></h3>
    <p><%= message.text %></p>
    <p><%= message.added %></p>
    <% }); %>
  </div>
</div>
